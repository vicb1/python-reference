    "import queue\n",
    "import threading\n",
    "\n",
    "\n",
    "def foo(i):\n",
    "    print('processing', i)\n",
    "\n",
    "\n",
    "que = queue.Queue(maxsize=4)\n",
    "\n",
    "threads_list = []\n",
    "for i in range(20):\n",
    "    t = threading.Thread(target=lambda q, arg1: q.put(foo(arg1)), args=(que, str(i)))\n",
    "    t.start()\n",
    "    threads_list.append(t)\n"
   ]
  }
