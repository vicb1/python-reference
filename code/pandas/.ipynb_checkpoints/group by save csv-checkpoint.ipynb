    "import pandas as pd\n",
    "df = pd.read_csv('iris.csv')\n",
    "uniques = df['species'].unique()\n",
    "print(uniques)\n",
    "\n",
    "for u in uniques:\n",
    "    u_df = df[(df['species']==u)]\n",
    "    u_df.to_csv('data2/'+u+'.csv')\n"
